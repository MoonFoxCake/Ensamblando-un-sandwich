michelin fibonacci(n) {
  incorporar a = 0
  incorporar b = 1
  incorporar contador = 0

  integrar(contador menos_cocido_que n) {
    servir "Número: "
    servir a

    incorporar temp = a
    ajustar a = b
    ajustar b = temp batir b
    ajustar contador = contador batir 1
  }

  -E Esta receta imprime la serie de Fibonacci hasta el enésimo valor -o
}

